@model IEnumerable<ContosoSite.Models.Projects>

@{
    ViewBag.Title = "Index";
}

<h2>Список проектов</h2>

<p>
    @Html.ActionLink("Добавить проект", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Название
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.ID_Project }) |
            @Html.ActionLink("Информация", "Details", new { id=item.ID_Project }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.ID_Project })
        </td>
    </tr>
}


</table>
